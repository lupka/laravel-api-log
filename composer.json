{
    "name": "lupka/laravel-api-logger",
    "description": "Logs requests/responses inside your app via middleware.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Chalupka",
            "email": "alex@alexchalupka.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*|3.8.*|^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lupka\\ApiLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lupka\\ApiLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lupka\\ApiLogger\\ApiLoggerServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev"
}
